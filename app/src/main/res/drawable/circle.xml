<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <item>
        <shape android:shape="oval">
            <solid android:color="?attr/colorOnSecondary" />
            <size
                android:width="12dp"
                android:height="12dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="?attr/android:background" />
            <size
                android:width="12dp"
                android:height="12dp" />
        </shape>
    </item>
</layer-list>